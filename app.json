{
  "expo": {
    "name": "Covid-19 Monitor",
    "slug": "covid19Monitor",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "App para visualização de dados atualizados sobre a situação atual do Covid-19 no Brasil",
    "githubUrl": "https://github.com/maiconcosta/Covid19Monitor"
  }
}
